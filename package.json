{
  "name": "vue-template",
  "version": "0.0.0",
  "description": "template of Vue.js",
  "private": true,
  "scripts": {
    "build": "run-p build:*",
    "build:html": "pug -o dist src",
    "build:js": "webpack",
    "watch": "webpack -w",
    "start": "webpack-dev-server --content-base dist",
    "verify": "run-p test lint",
    "test": "jest",
    "lint": "run-p lint:*",
    "lint:ts": "eslint ./src ./test --ext ts,vue",
    "lint:md": "markdownlint . --ignore ./node_modules",
    "lint:yaml": "yamllint .*.yml *.yml --ignore=node_modules/**/*.yml --ignore=node_modules/**/*.yaml"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:shimataro/vue-template.git"
  },
  "author": "shimataro",
  "devDependencies": {
    "@types/jest": "25.2.3",
    "@types/webpack": "4.41.13",
    "@typescript-eslint/eslint-plugin": "3.0.0",
    "@typescript-eslint/parser": "3.0.0",
    "css-loader": "3.5.3",
    "dotenv": "8.2.0",
    "eslint": "7.1.0",
    "eslint-plugin-vue": "6.2.2",
    "jest": "26.0.1",
    "markdownlint-cli": "0.23.1",
    "node-sass": "4.14.1",
    "npm-run-all": "4.1.5",
    "pug": "2.0.4",
    "pug-cli": "1.0.0-alpha6",
    "pug-plain-loader": "1.0.0",
    "sass-loader": "8.0.2",
    "style-loader": "1.2.1",
    "ts-jest": "26.0.0",
    "ts-loader": "7.0.5",
    "ts-node": "8.10.1",
    "typescript": "3.9.3",
    "vue": "2.6.11",
    "vue-eslint-parser": "7.1.0",
    "vue-loader": "15.9.2",
    "vue-template-compiler": "2.6.11",
    "webpack": "4.43.0",
    "webpack-cli": "3.3.11",
    "webpack-dev-server": "3.11.0",
    "yaml-lint": "1.2.4"
  }
}
