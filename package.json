{
  "name": "vue-template",
  "version": "0.0.0",
  "description": "template of Vue.js",
  "private": true,
  "scripts": {
    "build": "run-p build:*",
    "build:html": "pug -o dist src",
    "build:js": "webpack",
    "watch": "webpack -w",
    "start": "webpack-dev-server --content-base dist",
    "verify": "run-p test lint",
    "test": "jest",
    "lint": "run-p lint:*",
    "lint:ts": "eslint ./src ./test --ext ts,vue",
    "lint:md": "markdownlint . --ignore ./node_modules",
    "lint:yaml": "yamllint .*.yml *.yml --ignore=node_modules/**/*.yml --ignore=node_modules/**/*.yaml",
    "check-updates": "ncu"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:shimataro/vue-template.git"
  },
  "author": "shimataro",
  "devDependencies": {
    "@types/jest": "26.0.15",
    "@typescript-eslint/eslint-plugin": "4.6.1",
    "@typescript-eslint/parser": "4.6.1",
    "css-loader": "5.0.0",
    "dotenv": "8.2.0",
    "eslint": "7.12.1",
    "eslint-plugin-vue": "7.1.0",
    "jest": "26.6.2",
    "markdownlint-cli": "0.24.0",
    "node-sass": "7.0.0",
    "npm-check-updates": "9.2.3",
    "npm-run-all": "4.1.5",
    "pug": "3.0.0",
    "pug-cli": "1.0.0-alpha6",
    "pug-plain-loader": "1.0.0",
    "sass-loader": "10.0.5",
    "style-loader": "2.0.0",
    "ts-jest": "26.4.3",
    "ts-loader": "8.0.7",
    "ts-node": "9.0.0",
    "typescript": "4.0.5",
    "vue": "2.6.12",
    "vue-eslint-parser": "7.1.1",
    "vue-loader": "15.9.5",
    "vue-template-compiler": "2.6.12",
    "webpack": "5.3.2",
    "webpack-cli": "4.1.0",
    "webpack-dev-server": "3.11.0",
    "yaml-lint": "1.2.4"
  }
}
